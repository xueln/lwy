Blur\">\r\n        <p :class=\"{vali:pwdNull}\">密码不能为空</p>\r\n        <input @click=\"loginNow\"  type=\"button\" value=\"立即登录\" id=\"login\">\r\n        <p>\r\n            <input type=\"checkbox\" id=\"auto\">\r\n            <label for=\"auto\">30天内免登陆</label>\r\n            <a id=\"forget_pwd\" href=\"javascript:;\">忘记密码?</a>\r\n        </p>\r\n        <p class=\"other_login\">\r\n            <span class=\"left_line\"></span>\r\n            其他方式登录\r\n            <span class=\"right_line\"></span>\r\n        </p>\r\n        <ul class=\"three\">\r\n            <li>\r\n                <i class=\"qq_icn\"></i>\r\n                <a href=\"\">QQ</a>\r\n            </li>\r\n            <li class=\"sina\">\r\n                <i class=\"sina_icon\"></i>\r\n                <a href=\"\">新浪微博</a>\r\n            </li>\r\n            <li class=\"zfb\">\r\n                <i class=\"zfb_icon\"></i>\r\n                <a href=\"\">支付宝</a>\r\n            </li>\r\n        </ul>\r\n    </div>\r\n</template>\r\n<script>\r\nimport {mapActions,mapGetters} from 'vuex'\r\nexport default {\r\n    data(){\r\n        return{\r\n            myPhone:\"\",\r\n            myPwd:\"\",\r\n            lgFailText:true,\r\n            pNull:true,\r\n            pwdNull:true,\r\n            flag:false\r\n        }\r\n    },\r\n    methods:{\r\n        ...mapActions([\"userLogin\"]),\r\n        phoneBlur(e){\r\n            this.myPhone=this.myPhone.trim();\r\n            if(!this.myPhone){\r\n                this.pNull=false;\r\n            }else{\r\n                this.pNull=true;\r\n            }\r\n        },\r\n        pwdBlur(e){\r\n            this.myPwd=this.myPwd.trim();\r\n            if(!this.myPwd){\r\n                this.pwdNull=false;\r\n                this.flag=false;\r\n                \r\n            }else{\r\n                this.pwdNull=true;\r\n                this.flag=true;\r\n            }\r\n            \r\n            \r\n        },   \r\n        loginNow(){\r\n            console.log(this.myPwd);  \r\n           if(this.myPhone==\"\" ){\r\n               document.getElementById(\"phone\").focus();\r\n               return;\r\n           }else if(this.myPwd==\"\"){\r\n                document.getElementById(\"upwd\").focus();\r\n                return ;\r\n           }\r\n            // 登录 保留状态\r\n            (async ()=>{\r\n                await this.userLogin({\r\n                    iphone:this.myPhone,\r\n                    upwd:this.myPwd\r\n                });\r\n                if(this.getIsLogin){\r\n                    alert(\"登录成功\");\r\n                    this.$router.push('/');\r\n                }else{\r\n                    alert(\"用户名或密码错误\");\r\n                }\r\n                this.myPhone=\"\";\r\n                this.myPwd=\"\";\r\n                 \r\n            })();\r\n        },\r\n        goreg(){\r\n            this.$router.push('/Register');\r\n        }\r\n    },\r\n    computed:{\r\n        ...mapGetters([\"getIsLogin\"])\r\n    },\r\n    directives:{\r\n        \"autoFocus\":{\r\n            inserted(dom){\r\n                dom.focus();\r\n            }\r\n        }\r\n    }\r\n}\r\n</script>\r\n<style>\r\n    @import url(../assets/css/login.css);\r\n</style>\r\n\r\n"]}]}